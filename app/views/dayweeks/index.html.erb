<%= render 'layouts/body' %>
<% @dayweek.each do |dayweek| %>
<div>
    <div class="container">
          <div class="row">
            <div class="col">
            <a href="<%= dayweek_path(dayweek) %>"><h1 class = 'display-4'><%= dayweek.brief %></h1></a>
            </div>
            <div class="col-2"><br>
              <% if user_signed_in? %>  <%= link_to 'Редактировать', edit_dayweek_path(dayweek), {class: 'btn btn-secondary btn-sm'}  %><% end %>
            </div>
      </div>
  </div>
</div>
<% end %>